{
  "name": "@risingstack/graffiti-mongoose",
  "version": "5.1.0",
  "description": "Mongoose adapter for graffiti (Node.js GraphQL ORM)",
  "main": "lib/index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --compilers js:babel-register --require babel-polyfill 'src/**/*.spec.js'",
    "eslint": "eslint src",
    "prepublish": "npm run build",
    "build": "rm -rf lib/* && babel src --ignore *.spec.js --out-dir lib",
    "coverage": "NODE_ENV=test babel-istanbul cover _mocha -- --require babel-polyfill 'src/**/*.spec.js'"
  },
  "author": "RisingStack",
  "repository": {
    "type": "git",
    "url": "git@github.com:RisingStack/graffiti-mongoose.git"
  },
  "bugs": {
    "url": "https://github.com/RisingStack/graffiti-mongoose/issues"
  },
  "homepage": "https://github.com/RisingStack/graffiti-mongoose#readme",
  "license": "MIT",
  "keywords": [
    "GraphQL",
    "graffiti",
    "mongoose",
    "ORM",
    "Relay"
  ],
  "dependencies": {
    "graphql-relay": "^0.4.1",
    "lodash": "^4.11.1"
  },
  "peerDependencies": {
    "graphql": "^0.5.0",
    "babel-polyfill": "^6.5.0"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.7.7",
    "babel-eslint": "6.0.4",
    "babel-istanbul": "0.8.0",
    "babel-polyfill": "6.7.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.7.2",
    "chai": "3.5.0",
    "chai-subset": "1.2.2",
    "eslint": "2.9.0",
    "eslint-config-airbnb": "8.0.0",
    "eslint-plugin-import": "1.6.1",
    "graphql": "0.5.0",
    "mocha": "2.4.5",
    "mongoose": "4.4.14",
    "pre-commit": "1.1.2",
    "sinon": "1.17.3",
    "sinon-chai": "2.8.0"
  },
  "pre-commit": [
    "test",
    "eslint"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  }
}
